{
  "name": "wonlog-server",
  "version": "0.0.1",
  "description": "Simple analytic tool that streams local logs to web browser.",
  "main": "dist/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "wonlog-agent": "wonlog-agent",
    "wonlog-server": "dist/server_local/server.js"
  },
  "scripts": {
    "tsc": "tsc --noEmit",
    "build": "NODE_ENV=production tsc",
    "serve": "NODE_ENV=production node dist/index.js",
    "start1": "NODE_ENV=development nodemon --exec ts-node --files src/index.ts",
    "test": "NODE_ENV=test jest --forceExit --verbose --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Devrama/wonlog.git"
  },
  "keywords": [
    "log",
    "logs",
    "logging",
    "logio",
    "rtail",
    "monitoring",
    "realtime"
  ],
  "author": "WON JONG YOO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Devrama/wonlog/issues"
  },
  "homepage": "https://github.com/Devrama/wonlog#readme",
  "devDependencies": {
    "@types/body-parser": "1.19.0",
    "@types/compression": "1.7.0",
    "@types/errorhandler": "1.5.0",
    "@types/express": "4.17.9",
    "@types/lusca": "1.6.2",
    "@types/node": "14.14.13",
    "@types/ws": "7.4.0",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.9.1",
    "compression": "1.7.4",
    "concurrently": "5.3.0",
    "errorhandler": "1.5.1",
    "eslint": "7.15.0",
    "eslint-config-prettier": "7.0.0",
    "husky": "4.3.6",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "lusca": "1.6.1",
    "nodemon": "2.0.6",
    "prettier": "2.2.1",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "express": "4.17.1",
    "js-yaml": "3.14.1",
    "ws": "7.4.1"
  }
