{
  "name": "wonlog-agent",
  "version": "0.0.1",
  "description": "Simple analytic tool that streams local logs to web browser.",
  "engines": {
    "node": "14"
  },
  "main": "dist/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "wonlog-agent": "./dist/bin/agent.js"
  },
  "scripts": {
    "build": "NODE_ENV=production tsc",
    "serve": "NODE_ENV=production node dist/index.js",
    "start": "NODE_ENV=development ts-node --files bin/agent.ts",
    "test": "NODE_ENV=test jest --forceExit --verbose --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Devrama/wonlog.git"
  },
  "keywords": [
    "log",
    "logs",
    "logging",
    "logio",
    "rtail",
    "monitoring",
    "realtime"
  ],
  "author": "WON JONG YOO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Devrama/wonlog/issues"
  },
  "homepage": "https://github.com/Devrama/wonlog#readme",
  "devDependencies": {
    "@types/split2": "3.2.1"
  },
  "dependencies": {
    "commander": "8.1.0",
    "json5": "2.2.0",
    "nanoid": "3.1.25",
    "split2": "3.2.2"
  }
}
