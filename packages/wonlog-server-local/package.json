{
  "name": "wonlog-server",
  "version": "0.0.1",
  "description": "Simple analytic tool that streams local logs to web browser.",
  "engines": {
    "node": "14"
  },
  "main": "dist/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "wonlog-agent": "wonlog-agent",
    "wonlog-server": "dist/server_local/server.js"
  },
  "scripts": {
    "build": "NODE_ENV=production tsc",
    "serve": "NODE_ENV=production node dist/index.js",
    "start": "NODE_ENV=development mkdir -p dist/express_server/public && cp -r src/express_server/public/* dist/express_server/public && concurrently \"tsc -w\" \"nodemon dist/index.js\"",
    "test": "NODE_ENV=test jest --forceExit --verbose --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Devrama/wonlog.git"
  },
  "keywords": [
    "log",
    "logs",
    "logging",
    "logio",
    "rtail",
    "monitoring",
    "realtime"
  ],
  "author": "WON JONG YOO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Devrama/wonlog/issues"
  },
  "homepage": "https://github.com/Devrama/wonlog#readme",
  "devDependencies": {
    "@babel/cli": "7.15.4",
    "@babel/core": "7.15.5",
    "@babel/preset-env": "7.15.4",
    "@babel/preset-typescript": "7.15.0",
    "@types/body-parser": "1.19.1",
    "@types/compression": "1.7.1",
    "@types/errorhandler": "1.5.0",
    "@types/express": "4.17.13",
    "@types/lusca": "1.7.1",
    "@types/node": "14.17.14",
    "@types/ws": "7.4.7",
    "babel-loader": "8.2.2",
    "compression": "1.7.4",
    "concurrently": "6.2.1",
    "errorhandler": "1.5.1",
    "jest": "27.1.0",
    "lusca": "1.7.0",
    "nodemon": "2.0.12",
    "prettier": "2.3.2",
    "typescript": "4.4.2"
  },
  "dependencies": {
    "express": "4.17.1",
    "js-yaml": "4.1.0",
    "ws": "8.2.1"
  }
}
