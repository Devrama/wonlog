{
  "name": "wonlog-server",
  "version": "0.0.1",
  "description": "Simple analytic tool that streams local logs to web browser.",
  "main": "dist/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "wonlog-agent": "wonlog-agent",
    "wonlog-server": "dist/server_local/server.js"
  },
  "scripts": {
    "build": "NODE_ENV=production tsc",
    "serve": "NODE_ENV=production node dist/index.js",
    "start": "NODE_ENV=development mkdir -p dist/express_server/public && cp -r src/express_server/public/* dist/express_server/public && concurrently \"tsc -w\" \"nodemon dist/index.js\"",
    "test": "NODE_ENV=test jest --forceExit --verbose --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Devrama/wonlog.git"
  },
  "keywords": [
    "log",
    "logs",
    "logging",
    "logio",
    "rtail",
    "monitoring",
    "realtime"
  ],
  "author": "WON JONG YOO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Devrama/wonlog/issues"
  },
  "homepage": "https://github.com/Devrama/wonlog#readme",
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-typescript": "^7.7.2",
    "@types/body-parser": "^1.17.1",
    "@types/compression": "^1.0.1",
    "@types/errorhandler": "1.5.0",
    "@types/express": "^4.17.2",
    "@types/lusca": "^1.6.1",
    "@types/node": "^14.0.23",
    "@types/ws": "^6.0.4",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "babel-loader": "^8.0.6",
    "compression": "^1.7.4",
    "concurrently": "^5.0.0",
    "errorhandler": "^1.5.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.7.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.3",
    "lusca": "^1.6.1",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "express": "^4.17.1",
    "js-yaml": "^3.13.1",
    "ws": "^7.2.0"
  }
}
